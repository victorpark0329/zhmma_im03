					<!-- Attach Information -->
					<form:SimpleForm id="IOAttachDisplay">
						<form:content>
						<UploadCollection
							id="oUploadCollection"
							showNoData="false"
							maximumFilenameLength="500"
							maximumFileSize="20"
							multiple="true"
							sameFilenameAllowed="true"
							instantUpload="false"
							showSeparators="All"
							>
						</UploadCollection>
						</form:content>
					</form:SimpleForm>	
				</IconTabFilter>
				<IconTabFilter text="Summary" icon="sap-icon://image-viewer">
						<!-- Attach Information -->
						<form:SimpleForm id="IOImageDisplay">
							<form:content>
								<unified:FileUploader
									id="oImageUploader"
									name="myFileUpload"
									width="400px"
									tooltip="jpg,bmp,gif,tif,png file only"
									uploadComplete="handleUploadComplete"
									change="handleImageChange"
									typeMissmatch="zhmmaim.util.Commons.handleTypeMissmatch"
									style="Emphasized"
									fileType="jpg,bmp,gif,tif,png"
									placeholder="Choose a Image for Upload..."/>
							</form:content>
						</form:SimpleForm>
						<Image
							id="oImage"
							densityAware="false"
							width="1000px" >
						</Image>
					</IconTabFilter>
			</items>
		</IconTabBar>
			 
