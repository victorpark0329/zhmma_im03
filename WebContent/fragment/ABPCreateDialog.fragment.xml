						<Label text="GR" />
						<Label text="Install" />
						<Label text="Finish" required="true"/>
					</l:content>
					<l:content>
						<DatePicker id="GDATU" displayFormat="MM/dd/yyyy" valueFormat="yyyyMMdd" change="onDPchange"/>
						<DatePicker id="WDATU" displayFormat="MM/dd/yyyy" valueFormat="yyyyMMdd" change="onDPchange"/>
						<DatePicker id="PODATE" displayFormat="MM/dd/yyyy" valueFormat="yyyyMMdd" change="onDPchange"/>
