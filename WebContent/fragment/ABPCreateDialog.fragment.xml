		<!-- Detail Table Information -->
		<Panel>
			<headerToolbar>
				<Toolbar class="updatedToolBarHeaderTitle2">
					<Title text="Details"/>
				</Toolbar>
			</headerToolbar>
			<content>
				<form:SimpleForm	
						layout = "ResponsiveGridLayout" class="updatedItemBody"
						>
					<form:content>
						<table:Table id="oABPCreateItemTB" visibleRowCount="3" rows="{/}" >
						<table:title>
							<Toolbar>
								<Title text="Items" titleStyle="H4"/>
								<core:Icon src="add" press="zhmmaim.util.Commons.addTabRow"/>
							</Toolbar>
						</table:title>
							
						<table:columns>
							<table:Column>
								<Text text="Item"/>
								<table:template>
									<commons:TextField id="MATNR" value="{MATNR}"/>
								</table:template>
							</table:Column>
							<table:Column width="20rem">
								<Text text="Description" />
								<table:template>
									<commons:TextField id="MAKTX" value="{MAKTX}" width="20rem"/>
								</table:template>
							</table:Column>
							<table:Column>
								<Text text="Cost"/>
								<table:template>
									<commons:TextField id="PREIS" value="{PREIS}" liveChange="zhmmaim.util.Commons.onSum"/>
								</table:template>
							</table:Column>
							<table:Column>
								<Text text="Qty"/>
