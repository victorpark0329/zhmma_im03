								<suggestionColumns>
										<Column hAlign="Begin" popinDisplay="Inline" demandPopin="true">
											<Label text="Code"/>
										</Column>
										<Column hAlign="Begin" popinDisplay="Inline" demandPopin="true">
											<Label text="Info"/>
										</Column>
								</suggestionColumns>
								<suggestionRows>
									<ColumnListItem>
										<cells>
											<Label text="{INPUT}"/>
											<Label text="{TEXT}"/>
										</cells>
									</ColumnListItem>
								</suggestionRows>
						<layoutData>
							<l:GridData span="L1 M3 S3"/>
						</layoutData>
						</Input>
						<Input id="IVPROD" type="Text" value="{/IVPROD}" editable="false"/>
						
						<Label text="IM.Reason" required="true" class="sapUiSmallMarginTop"/>
						<Input id="IZWEK" type="Text" value="" required="true" maxSuggestionWidth="350px" showSuggestion="true" showValueHelp="true" suggestionRows="{/results}" change="zhmmaim.util.Commons.onDescription" liveChange="zhmmaim.util.Commons.onCallPossible" valueHelpRequest="zhmmaim.util.Commons.showAllList">
								<suggestionColumns>
										<Column hAlign="Begin" popinDisplay="Inline" demandPopin="true">
											<Label text="Code"/>
										</Column>
										<Column hAlign="Begin" popinDisplay="Inline" demandPopin="true">
											<Label text="Info"/>
										</Column>
								</suggestionColumns>
								<suggestionRows>
									<ColumnListItem>
										<cells>
											<Label text="{INPUT}"/>
											<Label text="{TEXT}"/>
										</cells>
									</ColumnListItem>
								</suggestionRows>
						<layoutData>
							<l:GridData span="L1 M3 S3"/>
						</layoutData>
						</Input>
						<Input id="IZWEKD" type="Text" value="{/IZWEKD}" editable="false"/>
						
						<Label text="Asset Class" required="true" class="sapUiSmallMarginTop"/>
						<!-- <Input id="ANLKL" type="Text" value="{/ANLKL}" required="true"> -->
						<Input id="ANLKL" type="Text" value="" required="true" maxSuggestionWidth="350px" showSuggestion="true" showValueHelp="true" suggestionRows="{/results}" change="zhmmaim.util.Commons.onDescription" liveChange="zhmmaim.util.Commons.onCallPossible" valueHelpRequest="zhmmaim.util.Commons.showAllList">
									<suggestionColumns>
											<Column hAlign="Begin" popinDisplay="Inline" demandPopin="true">
												<Label text="Code"/>
											</Column>
											<Column hAlign="Begin" popinDisplay="Inline" demandPopin="true">
												<Label text="Info"/>
											</Column>
									</suggestionColumns>
									<suggestionRows>
										<ColumnListItem>
											<cells>
												<Label text="{INPUT}"/>
												<Label text="{TEXT}"/>
											</cells>
										</ColumnListItem>
									</suggestionRows>
							<layoutData>
								<l:GridData span="L1 M2 S3"/>
							</layoutData>
						</Input>
						<Input id="ANLKLD" type="Text" value="{/ANLKLD}" editable="false"/>
						
						<Label text="Capital.Date" class="sapUiSmallMarginTop"/>
						<DatePicker id="AKTIV" displayFormat="MM/dd/yyyy" valueFormat="yyyyMMdd" change="onDPchange">
						<layoutData>
							<l:GridData span="L2 M3 S3"/>
						</layoutData>
						</DatePicker>
						<Label text="Asset Name" class="sapUiSmallMarginTop"/>
						<Input id="TXT50" type="Text" value="">
						<layoutData>
							<l:GridData span="L4 M4 S4"/>
						</layoutData>
						</Input>
						
						<Label text="Modification Asset" class="sapUiSmallMarginTop"/>
						<Switch id="ASSET_MOD" type="Default" state="false" class="sapUiSmallMarginBegin">
							<layoutData>
								<l:GridData span="L2 M2 S2"/>
							</layoutData>
						</Switch>
						
						<Text id="oExistAsset_LABEL" text="Existing Asset:" class="sapUiSmallMarginTop">
							<layoutData>
								<l:GridData span="L2 M2 S2"/>
							</layoutData>
						</Text>
						<MultiInput id="oExistAssestToken" placeholder="Input Asset.." showValueHelp="false">
							<layoutData>
								<l:GridData span="L3 M3 S3"/>
							</layoutData>
						</MultiInput>
						<Label text="Disposal of existing asset" class="sapUiSmallMarginTop"/>
						<RadioButtonGroup id="ASSET_DIS" columns="4" selectedIndex="1">
							<buttons>
								<RadioButton text="Yes"/>
								<RadioButton text="No"/>
							</buttons>
						</RadioButtonGroup>
			</form:content>
		</form:SimpleForm>							  	
		
