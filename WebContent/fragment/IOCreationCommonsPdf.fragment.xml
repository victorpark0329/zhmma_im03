<Page showHeader="false" class="hiddenPrintArea hiddenPrintPage">

			<VBox class="printHeaderBox">
				<HBox><Label text="EFFECT"/></HBox>
			</VBox>

			<VBox class="printBorderBox">
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Tangible" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBox">
							<TextArea row="3" width="100%" wrapping="Soft" class="printClassEFFTA"/>
						</HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Intangible" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBox">
							<TextArea  row="3" width="100%" wrapping="Soft" class="printClassEFFIN"/>
						</HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Location" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBox">
							<TextArea  row="3" width="100%" wrapping="Soft" class="printClassTPLNR"/>
						</HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Revenue" class="sapUiSmallMarginTop"/></HBox>
						<HBox>
							<HBox>
								<Text text="Payback Period" class="sapUiSmallMarginTop"/>
							</HBox>
							<HBox>
								<Input type="Text" editable="false" class="printEffectInputMarginTop printClassROI"/>
							</HBox>
						</HBox>
				</HBox>
				<HBox>
						<HBox width="100%">
							<table:Table id="printIOPayTB" />
						</HBox>
				</HBox>
			</VBox>

	</Page>
	//-->
</core:FragmentDefinition>
