<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout"
	xmlns:table="sap.ui.table" xmlns:commons="sap.ui.commons" xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="IOCreationCommonsPdf" showHeader="false" class="hiddenPrintArea">

		<html:style>
			.hiddenPrintArea {display:none}
			.printBorderBox {border: 1px solid #bbb; margin:2% 5% 2% 5%; padding: 2% 4% 2% 4%}
			.printLineTitle {text-align:right; valign:middle; padding-right: 1%}
			.printLineTitle .sapMFlexItem {width:100%}
			.printPaddingLeft {padding-left:1%}
			.printPaddingLeft2 {padding-left:2%}
			.printFullSizeBox {width:75%}
			.printFullSizeBox .sapMFlexItem {width:100%}
			.printFullSizeBox2 .sapMFlexItem {width:28%}

			.printFullSizeBox3Year {width:15%}
			.printFullSizeBox3POSID {width:40%}
			.printFullSizeBox3POSID .sapMFlexItem {width:100%}
			.printFullSizeBox3POSIDD {width:45%}

			.printFullSizeBox2, .printFullSizeBox3 {width:75%}

			.printFullSizeBox2 .sapMFlexItem .sapMActionSelect {width:100% !important}
			.printInput {width:50%}

			.printHeaderBox {background-color:#457aae; margin: 0 1% 0 1%; padding: 1% 1% 0.5% 1%}
			.printHeaderBox .sapMFlexBox .sapMFlexItem .sapMLabel {font-size:18px; font-weight:normal; color:#fff }

			.printFullSizeBoxIOBasic {width:75%}
			.printFullSizeBoxIOBasic .sapMFlexItem:nth-child(4n+1){width:45%}
			.printFullSizeBoxIOBasic .sapMFlexItem:nth-child(4n+2){width:55%}


			.printFullSizeBoxIOIM {width:75%}
			.printFullSizeBoxIOIM .sapMFlexItem:nth-child(4n+1){width:45%}
			.printFullSizeBoxIOIM .sapMFlexItem:nth-child(4n+2){width:55%}


			.printFullSizeBoxIOIM2 {width:75%}
			.printBoxIOAssetText {width:40%}
			.printBoxIOAssetBox, .printBoxIOAssetBox .sapMFlexItem {width:100%}

			.printEffectInputMarginTop {margin-top:6%}


			.printHeaderBar {background-color:#bbb; margin: 0 1% 1% 1%; padding: 1% 1% 0.5% 1%}

			.printHeaderBarLeft{width:50%}
			.printHeaderBarLeft .sapMFlexItem .sapMLabel {font-size:20px; font-weight:bold; color:#000 }
			.printHeaderBarRight{width:50%; justify-content: flex-end;}
			.printHeaderBarRight .sapMFlexItem .sapMLabel {font-size:20px; font-weight:bold; color:#000}


			.printLineBox {margin: 0 1% 1% 1%; padding: 2% 1% 2% 1%}
			.printLineBox .sapMFlexBox {width:100%;}
			.printFullSizeBoxIOIM2 .sapMFlexItem:nth-child(4n+2){margin-left:2%;margin-right:1%;margin-top:-1.5%}
			.printFullSizeBoxIOIM2 .sapMFlexItem:nth-child(4n+3){width:100%;margin-top:0.5%}


		</html:style>
	</Page>

	<Page showHeader="false" class="hiddenPrintArea hiddenPrintPage">

	<HBox class="printHeaderBar">
		<HBox class="printHeaderBarLeft">
			<Label text="{/BTSUBJ}"/>
		</HBox>
		<HBox class="printHeaderBarRight">
			<Label text="Document No."/>
			<Label id="BGTDOC" text="{/BGTDOC}"/>
		</HBox>
	</HBox>

		<HBox class="printLineBox">
			<!--
			<IconTabBar class="sapUiResponsiveContentPadding sapUiIOCreation">
				<items>
					<IconTabFilter text="WorkFlow" icon="sap-icon://manager" key="workflowInfo">
//-->
	<VBox class="sapUiIOCreation">
						<Table id="printCreateWF" showNoData="false" class="tblApprovalLine">
							<infoToolbar>
								<Toolbar>
									<Text text="Approval Line" level="H5"/>
								</Toolbar>
							</infoToolbar>
						</Table>
						<Table id="printIOCreateCP" showNoData="false" class="tblCooperationLine">
							<infoToolbar>
								<Toolbar>
									<Text text="Cooperation Line" level="H5"/>
								</Toolbar>
							</infoToolbar>
						</Table>
	</VBox>
<!--
					</IconTabFilter>
					<IconTabFilter text="Attachments" icon="sap-icon://attachment">
					</IconTabFilter>
					<IconTabFilter text="Image" icon="sap-icon://image-viewer">
					</IconTabFilter>
					<IconTabFilter text="Comment" icon="sap-icon://comment" key="commentInfo">
					</IconTabFilter>
				</items>
			</IconTabBar>
			//-->
		</HBox>

			<VBox class="printHeaderBox">
				<HBox><Label text="HEADER"/></HBox>
			</VBox>
			<VBox class="printBorderBox">
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Type" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBox2">
							<ActionSelect>
								<items>
									<core:Item key="A" text="With PR"/>
									<core:Item key="B" text="Without PR"/>
								</items>
							</ActionSelect>
							<commons:CheckBox text="Already Approved" class="sapUiSmallMarginTop printPaddingLeft2" />
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Est. Cost" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBox2">
						<Input type="Text" class="printInputSmall printClassUSER4" />
						<HBox width="0.5%"></HBox>
						<Select>
							<items>
								<core:Item key="USD" text="USD"/>
							</items>
						</Select>
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Position ID" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBox3">
							<HBox class="printFullSizeBox3Year"><Input type="Number" class="printClassYear" /></HBox>
							<HBox class="printFullSizeBox3POSID"><Input type="Text" class="printPaddingLeft2 printClassPOSID"/></HBox>
							<HBox class="printFullSizeBox3POSIDD"><Input type="Text" editable="false" class="printPaddingLeft2 printClassPOSIDD"/></HBox>
					</HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Description" required="true" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBox"><Input type="Text" class="printInput printClassKTEXT"/></HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Purpose" required="true" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBox">
							<TextArea row="3" width="100%" wrapping="Soft" class="printClassPURPO"/>
						</HBox>
				</HBox>
			</VBox>


			<VBox class="printHeaderBox">
				<HBox><Label text="IO-Basic"/></HBox>
			</VBox>
			<VBox class="printBorderBox">
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Plant" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBoxIOBasic">
						<Input type="Text" editable="true" class="printClassWERKS"/>
						<Input type="Text" editable="false" class="printClassWERKSD"/>
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="CC.Reqst" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBoxIOBasic">
						<Input type="Text" editable="true" class="printClassKOSTV"/>
						<Input type="Text" editable="false" class="printClassKOSTVD"/>
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="CC.Resp" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBoxIOBasic">
						<Input type="Text" editable="true" class="printClassAKSTL"/>
						<Input type="Text" editable="false" class="printClassAKSTLD"/>
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Applicant" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBoxIOBasic">
						<Input type="Text" editable="true" class="printClassUser0"/>
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Applicant Telephone" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBoxIOBasic">
						<Input type="Text" editable="true" class="printClassUser1"/>
					</HBox>
				</HBox>
				<HBox>
					<HBox width="25%" class="printLineTitle"><Label text="Person.Responsible" required="true" class="sapUiSmallMarginTop"/></HBox>
					<HBox class="printFullSizeBoxIOBasic">
						<Input type="Text" editable="true" class="printClassUser2"/>
					</HBox>
				</HBox>
			</VBox>

		</Page>
		<Page showHeader="false" class="hiddenPrintArea hiddenPrintPage">

			<VBox class="printHeaderBox">
				<HBox><Label text="IO-Schedule"/></HBox>
			</VBox>

			<VBox class="printBorderBox">
				<HBox>
						<HBox width="16%"><Label text="Approval" required="true"/></HBox>
						<HBox width="17%"><Label text="PR" required="true"/></HBox>
						<HBox width="17%"><Label text="PO" /></HBox>
						<HBox width="17%"><Label text="GR" /></HBox>
						<HBox width="17%"><Label text="Install" /></HBox>
						<HBox width="16%"><Label text="Finish" required="true"/></HBox>
				</HBox>
				<HBox>
						<Input class="printClassUSER5"/>
						<HBox width="1%" />
						<Input class="printClassUSER7"/>
						<HBox width="1%" />
						<Input class="printClassPODATE"/>
						<HBox width="1%" />
						<Input class="printClassGRDATE"/>
						<HBox width="1%" />
						<Input class="printClassINSDATE"/>
						<HBox width="1%" />
						<Input class="printClassUSER8"/>
				</HBox>
			</VBox>

			<VBox class="printHeaderBox">
				<HBox><Label text="IO-IM"/></HBox>
			</VBox>

			<VBox class="printBorderBox">
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="IM.Profile" required="true" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBoxIOIM">
							<Input type="Text" editable="true" class="printClassIVPRO"/>
							<Input type="Text" editable="false" class="printClassIVPROD"/>
						</HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="IM.Reason" required="true" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBoxIOIM">
							<Input type="Text" editable="true" class="printClassIZWEK"/>
							<Input type="Text" editable="false" class="printClassIZWEKD"/>
						</HBox>
				</HBox>
				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Asset Class" required="true" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBoxIOIM">
							<Input type="Text" class="printClassANLKL"/>
							<Input type="Text" editable="false" class="printClassANLKLD"/>
						</HBox>
				</HBox>

				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Capital.Date" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBoxIOIM">
							<Input type="Text" class="printClassCapitalDate"/>
						</HBox>
				</HBox>

				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Asset Name" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBox">
							<Input type="Text" class="printClassTXT50"/>
						</HBox>
				</HBox>


				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Modification Asset" class="sapUiSmallMarginTop"/></HBox>
						<HBox class="printFullSizeBoxIOIM2">

								<Switch type="Default" state="false" class="sapUiSmallMarginBegin">
								</Switch>

								<Text text="Existing Asset:" class="sapUiSmallMarginTop"/>

								<MultiInput showValueHelp="false" class="printClassExistingAsset" />

						</HBox>
				</HBox>

				<HBox>
						<HBox width="25%" class="printLineTitle"><Label text="Disposal of existing asset" class="sapUiSmallMarginTop"/></HBox>
						<HBox>
							<RadioButtonGroup columns="4" selectedIndex="1">
								<buttons>
									<RadioButton text="Yes" />
									<RadioButton text="No" />
								</buttons>
							</RadioButtonGroup>
						</HBox>
				</HBox>

			</VBox>

</Page>

